{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=vehicle_rental_db;Username=postgres;Password=postgres"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/"
  },
  "JwtSettings": {
    "SecretKey": "your-super-secret-key-that-is-at-least-32-characters-long-for-jwt-signing",
    "Issuer": "VehicleRental",
    "Audience": "VehicleRentalUsers",
    "ExpirationHours": 8
  },
  "FileStorage": {
    "BasePath": "uploads",
    "AllowedExtensions": [".png", ".bmp"],
    "MaxFileSize": 5242880
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
